{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Information"
    }
  },
  "AllowedHosts": "*",
  "ReverseProxy": {
    "Routes": {
      "orders": {
        "ClusterId": "orders",
        "Match": {
          "Path": "/order/{**catch-all}"
        },
        "Transforms": [
        ]
      },
      "recipes": {
        "ClusterId": "recipes",
        "Match": {
          "Path": "/recipes/{**catch-all}"
        },
        "Transforms": [
        ]
      },
      "kitchen": {
        "ClusterId": "kitchen",
        "Match": {
          "Path": "/kitchen/{**catch-all}"
        },
        "Transforms": [
        ]
      },
      "delivery": {
        "ClusterId": "delivery",
        "Match": {
          "Path": "/delivery/{**catch-all}"
        },
        "Transforms": [
        ]
      }
    },
    "Clusters": {
      "orders": {
        "Destinations": {
          "instance-one": {
            "Address": "http://order-manager-api"
          }
        }
      },
      "kitchen": {
        "Destinations": {
          "instance-one": {
            "Address": "http://kitchen-api"
          }
        }
      },
      "recipes": {
        "Destinations": {
          "instance-one": {
            "Address": "http://recipe-api"
          }
        }
      },
      "delivery": {
        "Destinations": {
          "instance-one": {
            "Address": "http://delivery-api"
          }
        }
      }
    }
  }
}